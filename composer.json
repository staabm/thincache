{
	"name": "staabm/thincache",
	"license": "MIT",
	"description": "Thin abstraction layer for PHP 5.2+",
	"authors": [
		{
			"name": "Markus Staab",
			"email": "markus.staab@redaxo.de"
		}
	],
	"require": {
		"php": "^7.0 || ^8.0"
	},
	"autoload": {
		"classmap": [
			"src/"
		]
	},
	"scripts": {
		"phpstan-baseline": [
			"phpstan analyse -c phpstan.neon.dist --generate-baseline || true"
		]
	},
	"require-dev": {
		"phpstan/phpstan": "^0.12.99"
	}
}
